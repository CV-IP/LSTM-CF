net: "models/deeplab/deeplab_lstm_v4_concat.prototxt"

# covering the full 10,000 testing images.
test_iter: 5050
# Carry out testing every 500 training iterations.
test_interval: 10000

base_lr: 1e-4
lr_policy: "step"
gamma: 0.1

stepsize: 20000
display: 1
max_iter: 100000
momentum: 0.9
weight_decay: 0.0005

snapshot: 10000
snapshot_prefix: "../project/2016-3-4/caffe_deeplab_lstm_v4_concat_train"
solver_mode: GPU
